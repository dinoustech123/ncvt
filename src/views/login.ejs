<%- include("../components/header") %>

  <section class="ncvt-login-section">
    <div class="container">
      <div class="row">
        <div class="col">
          <div class="ncvt-login_form">
            <form action="login" method="POST">
              <div class="ncvt-login-head">
                <span>Login Form</span>
              </div>
              <label for="">User Name</label>
              <div class="input-group mb-4">
                <span class="input-group-text" id="basic-addon1"><i class="bi bi-person-fill"></i></span>
                <input type="text" class="form-control form_control" name="branch_code" id="branch_code"
                  placeholder="User Name" aria-label="branch_code" aria-describedby="basic-addon1" required>
              </div>

              <label for="">Password</label>
              <div class="input-group mb-2">
                <span class="input-group-text" id="basic-addon1"><i class="bi bi-key-fill"></i></span>
                <input type="password" class="form-control form_control" name="password" id="password"
                  placeholder="Password" aria-label="Username" aria-describedby="basic-addon1" required>
              </div>
              <p>
                <%= message ? message?.error : "" %>
              </p>
              <div class=" sing_bttn mt-5">
                <button onclick="openNewPage()" type="submit">SIGN IN</button>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>



  </section>




  <%- include("../components/footer") %>