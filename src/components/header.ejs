<body>

  <section class=" top-section p-2">
    <div class="container">
      <div class="container">
        <div class="col-12 d-flex align-items-center justify-content-between">
          <div class="firm-info">
            <img src="/image/handshake-icon.png" alt="">
            <span>
              <b>Franchise Enquiry &nbsp; 9006141311 &nbsp; | &nbsp; 9955521884</b>
            </span>
          </div>
          <% if(!data){ %>
            <div class="firm-form-bttns">
              <ul class=" m-0 p-0 d-flex align-items-center gap-3">
                <li><a href="student-login">Student Login</a></li>
                <li><a class="bg-color" href="login">Center Login</a></li>
                <li><a class="fr-bg-color" href="associate-login">Associate Member Login</a></li>

              </ul>
            </div>
            <% }else { %>
              <% if(data?.studentId){ %>
                <div class=""
                  style="display: flex;flex-direction: row; gap: 1rem; color: rgb(255, 255, 255); font-size: 1.3rem;">

                  <p class="">
                    <%= data?.student_name?.toUpperCase() %>
                  </p>
                  <p><a href="student-portal">Dashboard</a></p>
                  <p><a href="studentLogut">Logout</a></p>
                </div>
                <% }else { %>
                  <% if(data.role=="associate" ) { %>
                    <div class=""
                      style="display: flex;flex-direction: row; gap: 1rem; color: rgb(255, 255, 255); font-size: 1.3rem;">
                      <p class="">
                        <%= data.name?.toUpperCase() %>
                      </p>
                      <p>Wallet :<%= data.userbalance?.balance %>
                      </p>
                      <p><a href="associate-dashboard">Dashboard</a></p>
                      <p><a href="logoutUser">Logout</a></p>
                    </div>
                    <% } else {%>
                      <div class=""
                        style="display: flex;flex-direction: row; gap: 1rem; color: rgb(255, 255, 255); font-size: 1.3rem;">
                        <p class="">
                          <%= data.branch_name?.toUpperCase() %>
                        </p>
                        <p>Wallet :<%= data.userbalance?.balance %>
                        </p>
                        <p><a href="branch_dashboard">Dashboard</a></p>
                        <p><a href="logoutUser">Logout</a></p>
                      </div>
                      <% } %>
                        <% } %>
                          <% } %>
        </div>
      </div>
    </div>
  </section>
  <section class="mid-logo-img-section pt-2 pb-2">
    <div class="container">
      <div class="row">
        <div class="col-12 d-flex align-items-center justify-content-between">
          <div class="firm-logo-img">
            <a class=" m-0" href="/api/home">
              <img src="/image/ncvt-logo01.png" alt="">
            </a>
          </div>

          <div class=" firm-img">
            <img src="/image/trusty-logo-img.jpg" alt="">
          </div>
        </div>
      </div>
    </div>
  </section>
